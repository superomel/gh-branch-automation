{
  "name": "branch-automation",
  "description": "NBA Engine services",
  "author": "",
  "license": "ISC",
  "workspaces": [
    "*"
  ],
  "private": true,
  "repository": {
    "type": "git",
    "url": "git@github.com:superomel/gh-branch-automation.git"
  },
  "devDependencies": {
    "semantic-release": "^24.2.7"
  },
  "release": {
    "debug": true,
    "branches": [
      "production"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/github",
        {
          "successCommentCondition": false,
          "failComment": false
        }
      ]
    ]
  },
  "dependencies": {
    "yarn": "^1.22.22"
  }
}
